  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "isTranslatable";
  rdfs:comment "Indicates whether content is translatable (True) or not (False). An example of something that is not translatable would be a MNEMONIC of an object or a number."@en;
  rdfs:domain cdi:ComplexLanguageString;
  rdfs:range xsd:boolean;
.

cdi:ComplexLanguageString-isTranslated
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "isTranslated";
  rdfs:comment "Indicates whether content is a translation (True) or an original (False)."@en;
  rdfs:domain cdi:ComplexLanguageString;
  rdfs:range xsd:boolean;
.

cdi:ComplexLanguageString-scope
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "scope";
  rdfs:comment "Supports specification of scope for the contained content. Use with the language specification to filter application of content."@en;
  rdfs:domain cdi:ComplexLanguageString;
  rdfs:range xsd:string;
.

cdi:ComplexLanguageString-structureUsed
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "structureUsed";
  rdfs:comment "The structure type used. Examples are HTML or restructured text."@en;
  rdfs:domain cdi:ComplexLanguageString;
  rdfs:range cdi:ControlledVocabularyEntry;
.

cdi:ComplexLanguageString-translationDate
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "translationDate";
  rdfs:comment "The date the content was translated. Provision of translation date allows user to verify if translation was available during data collection or other time linked activity."@en;
  rdfs:domain cdi:ComplexLanguageString;
  rdfs:range xsd:date;
.

cdi:ComplexLanguageString-translationSourceLanguage
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "translationSourceLanguage";
  rdfs:comment "List the language code of the source. Repeat of multiple language sources are used."@en;
  rdfs:domain cdi:ComplexLanguageString;
  rdfs:range xsd:language;
.


# datatype ContactInformation
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::ContactInformation
cdi:ContactInformation
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "ContactInformation";
  rdfs:comment "Definition\n============\nContact information for the individual or organization including location specification, address, web site, phone numbers, and other means of communication access. Address, location, telephone, and other means of communication can be repeated to express multiple means of a single type or change over time. Each major piece of contact information contains the element effectiveDates in order to date stamp the period for which the information is valid."@en;
  
  owl:hasKey (cdi:ContactInformation-address cdi:ContactInformation-email cdi:ContactInformation-emessaging cdi:ContactInformation-telephone cdi:ContactInformation-website);
.

cdi:ContactInformation-address
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "address";
  rdfs:comment "The address for contact."@en;
  rdfs:domain cdi:ContactInformation;
  rdfs:range cdi:Address;
.

cdi:ContactInformation-email
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "email";
  rdfs:comment "Email contact information."@en;
  rdfs:domain cdi:ContactInformation;
  rdfs:range cdi:Email;
.

cdi:ContactInformation-emessaging
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "emessaging";
  rdfs:comment "Electronic messaging other than email."@en;
  rdfs:domain cdi:ContactInformation;
  rdfs:range cdi:ElectronicMessageSystem;
.

cdi:ContactInformation-telephone
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "telephone";
  rdfs:comment "Telephone for contact."@en;
  rdfs:domain cdi:ContactInformation;
  rdfs:range cdi:Telephone;
.

cdi:ContactInformation-website
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "website";
  rdfs:comment "The URL of the Agent's website."@en;
  rdfs:domain cdi:ContactInformation;
  rdfs:range cdi:WebLink;
.


# datatype ControlledVocabularyEntry
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::ControlledVocabularyEntry
cdi:ControlledVocabularyEntry
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "ControlledVocabularyEntry";
  rdfs:comment "Definition\n============\nAllows for unstructured content which may be an entry from an externally maintained controlled vocabulary.If the content is from a controlled vocabulary provide the code value of the entry, as well as a reference to the controlled vocabulary from which the value is taken. Provide as many of the identifying attributes as needed to adequately identify the controlled vocabulary. Note that DDI has published a number of controlled vocabularies applicable to several locations using the external controlled vocabulary entry structure. If the code portion of the controlled vocabulary entry is language specific (i.e. a list of keywords or subject headings) use language to specify that language. In most cases the code portion of an entry is not language specific although the description and usage may be managed in one or more languages. Use of shared controlled vocabularies helps support interoperability and machine actionability."@en;
  
  owl:hasKey (cdi:ControlledVocabularyEntry-entryReference cdi:ControlledVocabularyEntry-entryValue cdi:ControlledVocabularyEntry-name cdi:ControlledVocabularyEntry-valueForOther cdi:ControlledVocabularyEntry-vocabulary);
.

cdi:ControlledVocabularyEntry-entryReference
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "entryReference";
  rdfs:comment "A reference to the specific item in the vocabulary referenced in the vocabulary attribute, using a URI or other resolvable identifier."@en;
  rdfs:domain cdi:ControlledVocabularyEntry;
  rdfs:range cdi:Reference;
.

cdi:ControlledVocabularyEntry-entryValue
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "entryValue";
  rdfs:comment "The value of the entry of the controlled vocabulary. If no controlled vocabulary is used the term is entered here and none of the properties defining the controlled vocabulary location are used."@en;
  rdfs:domain cdi:ControlledVocabularyEntry;
  rdfs:range xsd:string;
.

cdi:ControlledVocabularyEntry-name
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "name";
  rdfs:comment "The name of the code list (controlled vocabulary)."@en;
  rdfs:domain cdi:ControlledVocabularyEntry;
  rdfs:range xsd:string;
.

cdi:ControlledVocabularyEntry-valueForOther
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "valueForOther";
  rdfs:comment "If the value of the string is \"Other\" or the equivalent from the codelist, this attribute can provide a more specific value not found in the codelist."@en;
  rdfs:domain cdi:ControlledVocabularyEntry;
  rdfs:range xsd:string;
.

cdi:ControlledVocabularyEntry-vocabulary
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "vocabulary";
  rdfs:comment "A reference to the external controlled vocabulary, using a URI or other resolvable identifier."@en;
  rdfs:domain cdi:ControlledVocabularyEntry;
  rdfs:range cdi:Reference;
.


# datatype CorrespondenceDefinition
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::CorrespondenceDefinition
cdi:CorrespondenceDefinition
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "CorrespondenceDefinition";
  rdfs:comment "Definition\n============\nDescribes the commonalities and differences between two members using a textual description of both commonalities and differences plus an optional coding of the type of commonality."@en;
  
  owl:hasKey (cdi:CorrespondenceDefinition-commonality cdi:CorrespondenceDefinition-commonalityCode cdi:CorrespondenceDefinition-difference cdi:CorrespondenceDefinition-matching);
.

cdi:CorrespondenceDefinition-commonality
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "commonality";
  rdfs:comment "A description of the common features of the two items. Supports multiple language versions of the same content as well as optional formatting of the content."@en;
  rdfs:domain cdi:CorrespondenceDefinition;
  rdfs:range cdi:InternationalString;
.

cdi:CorrespondenceDefinition-commonalityCode
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "commonalityCode";
  rdfs:comment "Commonality expressed as a term or code. Supports the use of an external controlled vocabulary. If repeated, clarify each external controlled vocabulary used."@en;
  rdfs:domain cdi:CorrespondenceDefinition;
  rdfs:range cdi:ControlledVocabularyEntry;
.

cdi:CorrespondenceDefinition-difference
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "difference";
  rdfs:comment "A description of the differences between the two items. Supports multiple language versions of the same content as well as optional formatting of the content."@en;
  rdfs:domain cdi:CorrespondenceDefinition;
  rdfs:range cdi:InternationalString;
.

cdi:CorrespondenceDefinition-matching
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "matching";
  rdfs:comment "Allows specification of exact match, close match, or disjoint. These relationships can be further defined by describing commonalities or differences or providing additional controlled vocabulary descriptions of relationships."@en;
  rdfs:domain cdi:CorrespondenceDefinition;
  rdfs:range cdi:MatchingCriterion;
.


# datatype DateRange
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::DateRange
cdi:DateRange
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "DateRange";
  rdfs:comment "Definition\n============\nExpresses a date/time range using a start date and end date (both with the structure of Date and supporting the use of ISO and non-ISO date structures). Use in all locations where a range of dates is required, i.e. validFor, embargoPeriod, collectionPeriod, etc."@en;
  
  owl:hasKey (cdi:DateRange-endDate cdi:DateRange-startDate);
.

cdi:DateRange-endDate
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "endDate";
  rdfs:comment "The date (time) designating the end of the period or range."@en;
  rdfs:domain cdi:DateRange;
  rdfs:range cdi:CombinedDate;
.

cdi:DateRange-startDate
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "startDate";
  rdfs:comment "The date (time) designating the beginning of the period or range."@en;
  rdfs:domain cdi:DateRange;
  rdfs:range cdi:CombinedDate;
.


# datatype ElectronicMessageSystem
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::ElectronicMessageSystem
cdi:ElectronicMessageSystem
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "ElectronicMessageSystem";
  rdfs:comment "Definition\n============\nAny non-email means of relaying a message electronically. This would include text messaging, Skype, Twitter, ICQ, or other emerging means of electronic message conveyance. \n\nExamples\n==========\nSkype account, etc."@en;
  
  owl:hasKey (cdi:ElectronicMessageSystem-contactAddress cdi:ElectronicMessageSystem-effectiveDates cdi:ElectronicMessageSystem-isPreferred cdi:ElectronicMessageSystem-privacy cdi:ElectronicMessageSystem-typeOfService);
.

cdi:ElectronicMessageSystem-contactAddress
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "contactAddress";
  rdfs:comment "Account identification for contacting."@en;
  rdfs:domain cdi:ElectronicMessageSystem;
  rdfs:range xsd:string;
.

cdi:ElectronicMessageSystem-effectiveDates
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "effectiveDates";
  rdfs:comment "Time period during which the account is valid."@en;
  rdfs:domain cdi:ElectronicMessageSystem;
  rdfs:range cdi:DateRange;
.

cdi:ElectronicMessageSystem-isPreferred
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "isPreferred";
  rdfs:comment "Set to True if this is the preferred address."@en;
  rdfs:domain cdi:ElectronicMessageSystem;
  rdfs:range xsd:boolean;
.

cdi:ElectronicMessageSystem-privacy
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "privacy";
  rdfs:comment "Specify the level privacy for the address as public, restricted, or private. Supports the use of an external controlled vocabulary."@en;
  rdfs:domain cdi:ElectronicMessageSystem;
  rdfs:range cdi:ControlledVocabularyEntry;
.

cdi:ElectronicMessageSystem-typeOfService
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "typeOfService";
  rdfs:comment "Indicates the type of service used. Supports the use of a controlled vocabulary."@en;
  rdfs:domain cdi:ElectronicMessageSystem;
  rdfs:range cdi:ControlledVocabularyEntry;
.


# datatype Email
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::Email
cdi:Email
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "Email";
  rdfs:comment "Definition\n============\nAn e-mail address which conforms to the internet format (RFC 822) including its type and time period for which it is valid.\n\nExamples\n==========\ninfo@ddialliance.org; ex.ample@somewhere.org"@en;
  
  owl:hasKey (cdi:Email-effectiveDates cdi:Email-internetEmail cdi:Email-isPreferred cdi:Email-privacy cdi:Email-typeOfEmail);
.

cdi:Email-effectiveDates
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "effectiveDates";
  rdfs:comment "Time period for which the e-mail address is valid."@en;
  rdfs:domain cdi:Email;
  rdfs:range cdi:DateRange;
.

cdi:Email-internetEmail
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "internetEmail";
  rdfs:comment "The email address expressed as a string (should follow the Internet format specification - RFC 5322) e.g. user@server.ext, more complex and flexible examples are also supported by the format."@en;
  rdfs:domain cdi:Email;
  rdfs:range xsd:string;
.

cdi:Email-isPreferred
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "isPreferred";
  rdfs:comment "Set to True if this is the preferred email."@en;
  rdfs:domain cdi:Email;
  rdfs:range xsd:boolean;
.

cdi:Email-privacy
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "privacy";
  rdfs:comment "Indicates the level of privacy."@en;
  rdfs:domain cdi:Email;
  rdfs:range cdi:ControlledVocabularyEntry;
.

cdi:Email-typeOfEmail
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "typeOfEmail";
  rdfs:comment "Code indicating the type of e-mail address. Supports the use of an external controlled vocabulary. (e.g. home, office)."@en;
  rdfs:domain cdi:Email;
  rdfs:range cdi:ControlledVocabularyEntry;
.


# datatype EmbargoInformation

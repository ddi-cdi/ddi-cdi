  rdfs:comment "Date the record was last revised."@en;
  rdfs:domain cdi:ProvenanceInformation;
  rdfs:range xsd:date;
.


# datatype RationaleDefinition
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::RationaleDefinition
cdi:RationaleDefinition
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "RationaleDefinition";
  rdfs:comment "Definition\n============\nTextual description of the rationale/purpose for the version change and a coded value to provide an internal processing flag within and organization or system."@en;
  
  owl:hasKey (cdi:RationaleDefinition-rationaleCode cdi:RationaleDefinition-rationaleDescription);
.

cdi:RationaleDefinition-rationaleCode
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "rationaleCode";
  rdfs:comment "Rationale  ode is primarily for internal processing flags within an organization or system. Supports the use of an external controlled vocabulary."@en;
  rdfs:domain cdi:RationaleDefinition;
  rdfs:range cdi:ControlledVocabularyEntry;
.

cdi:RationaleDefinition-rationaleDescription
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "rationaleDescription";
  rdfs:comment "Textual description of the rationale/purpose for the version change to inform users as to the extent and implication of the version change. May be expressed in multiple languages."@en;
  rdfs:domain cdi:RationaleDefinition;
  rdfs:range cdi:InternationalString;
.


# datatype Reference
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::Reference
cdi:Reference
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "Reference";
  rdfs:comment "Definition\n============\nProvides a way of pointing to resources outside of the information described in the set of DDI-CDI metadata."@en;
  
  owl:hasKey (cdi:Reference-ddiReference cdi:Reference-deepLink cdi:Reference-description cdi:Reference-location cdi:Reference-nonDdiReference cdi:Reference-semantic cdi:Reference-uri cdi:Reference-validType);
.

cdi:Reference-ddiReference
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "ddiReference";
  rdfs:comment "A DDI type reference to a DDI object."@en;
  rdfs:domain cdi:Reference;
  rdfs:range cdi:InternationalRegistrationDataIdentifier;
.

cdi:Reference-deepLink
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "deepLink";
  rdfs:comment "The selector refers to the object identifier by the ddiReference and has deep linking purposes."@en;
  rdfs:domain cdi:Reference;
  rdfs:range cdi:Selector;
.

cdi:Reference-description
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "description";
  rdfs:comment "Human-readable description of the reference."@en;
  rdfs:domain cdi:Reference;
  rdfs:range xsd:string;
.

cdi:Reference-location
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "location";
  rdfs:comment "The location of the referenced resource, as appropriate to support retrieval."@en;
  rdfs:domain cdi:Reference;
  rdfs:range cdi:InternationalString;
.

cdi:Reference-nonDdiReference
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "nonDdiReference";
  rdfs:comment "A non-DDI reference to any object using a system of identification which is not supported by a URI."@en;
  rdfs:domain cdi:Reference;
  rdfs:range cdi:NonDdiIdentifier;
.

cdi:Reference-semantic
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "semantic";
  rdfs:comment "External qualifier to describe the purpose or meaning of the reference."@en;
  rdfs:domain cdi:Reference;
  rdfs:range cdi:ControlledVocabularyEntry;
.

cdi:Reference-uri
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "uri";
  rdfs:comment "A URI to any object."@en;
  rdfs:domain cdi:Reference;
  rdfs:range xsd:anyURI;
.

cdi:Reference-validType
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "validType";
  rdfs:comment "The expected type of the reference (e.g., the class or element according to the schema of the referenced resource)."@en;
  rdfs:domain cdi:Reference;
  rdfs:range xsd:string;
.


# datatype Selector
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::Selector
cdi:Selector
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "Selector";
  rdfs:comment "Definition\n==========\nA resource which describes the segment of interest in a representation of a resource. This class is not used directly, only its subclasses. It is defined accordingly the related selector of the Web Annotation Vocabulary, see https://www.w3.org/TR/annotation-vocab/#selector."@en;
  
  owl:hasKey ();
.


# datatype SpatialCoordinate
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::SpatialCoordinate
cdi:SpatialCoordinate
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "SpatialCoordinate";
  rdfs:comment "Definition\n============\nLists the value and format type for the coordinate value. Note that this is a single value (X coordinate or Y coordinate) rather than a coordinate pair."@en;
  
  owl:hasKey (cdi:SpatialCoordinate-content cdi:SpatialCoordinate-coordinateType);
.

cdi:SpatialCoordinate-content
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "content";
  rdfs:comment "The value of the coordinate expressed as a string."@en;
  rdfs:domain cdi:SpatialCoordinate;
  rdfs:range xsd:string;
.

cdi:SpatialCoordinate-coordinateType
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "coordinateType";
  rdfs:comment "Identifies the type of point coordinate system using a controlled vocabulary. Point formats include decimal degree, degrees minutes seconds, decimal minutes, meters, and feet."@en;
  rdfs:domain cdi:SpatialCoordinate;
  rdfs:range cdi:PointFormat;
.


# datatype SpatialPoint
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::SpatialPoint
cdi:SpatialPoint
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "SpatialPoint";
  rdfs:comment "Definition\n============\nA geographic point consisting of an X and Y coordinate. Each coordinate value is expressed separately providing its value and format."@en;
  
  owl:hasKey (cdi:SpatialPoint-xCoordinate cdi:SpatialPoint-yCoordinate);
.

cdi:SpatialPoint-xCoordinate
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "xCoordinate";
  rdfs:comment "An X coordinate (latitudinal equivalent) value and format expressed using the Spatial Coordinate structure."@en;
  rdfs:domain cdi:SpatialPoint;
  rdfs:range cdi:SpatialCoordinate;
.

cdi:SpatialPoint-yCoordinate
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "yCoordinate";
  rdfs:comment "A Y coordinate (longitudinal equivalent) value and format expressed using the Spatial Coordinate structure."@en;
  rdfs:domain cdi:SpatialPoint;
  rdfs:range cdi:SpatialCoordinate;
.


# datatype SpecializationRole
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::SpecializationRole
cdi:SpecializationRole
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "SpecializationRole";
  rdfs:comment "Definition\n============\nSpecific roles played by represented variables in terms of time, geography, and other concepts which are important for the harmonization and integration of data."@en;
  
  owl:hasKey ();
.


# datatype Statistic
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::Statistic
cdi:Statistic
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "Statistic";
  rdfs:comment "Definition\n============\nThe value of the statistic expressed as a decimal, float and/or double. Indicates whether it is weighted value and the computation base."@en;
  
  owl:hasKey (cdi:Statistic-computationBase cdi:Statistic-content cdi:Statistic-isWeighted cdi:Statistic-typeOfNumericValue);
.

cdi:Statistic-computationBase
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "computationBase";
  rdfs:comment "Defines the cases included in determining the statistic. The options are total = all cases, valid and missing (invalid); validOnly = Only valid values, missing (invalid) are not included in the calculation; missingOnly = Only missing (invalid) cases included in the calculation."@en;
  rdfs:domain cdi:Statistic;
  rdfs:range cdi:ComputationBaseList;
.

cdi:Statistic-content
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "content";
  rdfs:comment "The value of the statistic expressed as a real number."@en;
  rdfs:domain cdi:Statistic;
  rdfs:range xsd:double;
.

cdi:Statistic-isWeighted
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "isWeighted";
  rdfs:comment "Set to True if the statistic is weighted."@en;
  rdfs:domain cdi:Statistic;
  rdfs:range xsd:boolean;
.

cdi:Statistic-typeOfNumericValue
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "typeOfNumericValue";
  rdfs:comment "Indicate the type of numeric value as decimal, float, double."@en;
  rdfs:domain cdi:Statistic;
  rdfs:range xsd:string;
.


# datatype StructureSpecification
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::StructureSpecification
cdi:StructureSpecification
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "StructureSpecification";
  rdfs:comment "Definition\n============\nThe mathematical properties of the structure."@en;
  
  owl:hasKey (cdi:StructureSpecification-reflexive cdi:StructureSpecification-symmetric cdi:StructureSpecification-transitive);
.

cdi:StructureSpecification-reflexive
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "reflexive";
  rdfs:comment "Members of the selected scope of the collection are related to themselves."@en;
  rdfs:domain cdi:StructureSpecification;
  rdfs:range cdi:MemberRelationshipScope;
.

cdi:StructureSpecification-symmetric
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "symmetric";
  rdfs:comment "For pairs of members, a, b in the indicated scope of the associated collection, whenever a is related to b then also b is related to a."@en;
  rdfs:domain cdi:StructureSpecification;
  rdfs:range cdi:MemberRelationshipScope;
.

cdi:StructureSpecification-transitive
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "transitive";
  rdfs:comment "For members a, b, c in the indicated scope of the associated collection, whenever a is related to b and b is related to c then a is also related to c."@en;
  rdfs:domain cdi:StructureSpecification;
  rdfs:range cdi:MemberRelationshipScope;
.


# datatype Telephone
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::Telephone
cdi:Telephone
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "Telephone";
  rdfs:comment "Definition\n============\nDetails of a telephone number including the number, type of telephone number, a privacy setting and an indication of whether this is the preferred contact number.\n\nExamples\n==========\n+12 345 67890123"@en;
  
  owl:hasKey (cdi:Telephone-effectiveDates cdi:Telephone-isPreferred cdi:Telephone-privacy cdi:Telephone-telephoneNumber cdi:Telephone-typeOfTelephone);
.

cdi:Telephone-effectiveDates
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "effectiveDates";
  rdfs:comment "Time period during which the telephone number is valid."@en;
  rdfs:domain cdi:Telephone;
  rdfs:range cdi:DateRange;
.

cdi:Telephone-isPreferred
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "isPreferred";
  rdfs:comment "Set to True if this is the preferred telephone number for contact."@en;
  rdfs:domain cdi:Telephone;
  rdfs:range xsd:boolean;
.

cdi:Telephone-privacy
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "privacy";
  rdfs:comment "Specify the level privacy for the telephone number as public, restricted, or private. Supports the use of an external controlled vocabulary."@en;
  rdfs:domain cdi:Telephone;
  rdfs:range cdi:ControlledVocabularyEntry;
.

cdi:Telephone-telephoneNumber
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "telephoneNumber";
  rdfs:comment "The telephone number including country code if appropriate."@en;
  rdfs:domain cdi:Telephone;
  rdfs:range xsd:string;
.

cdi:Telephone-typeOfTelephone
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "typeOfTelephone";
  rdfs:comment "Indicates type of telephone number provided (home, fax, office, cell, etc.). Supports the use of a controlled vocabulary."@en;
  rdfs:domain cdi:Telephone;
  rdfs:range cdi:ControlledVocabularyEntry;
.


# datatype TextPositionSelector
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::TextPositionSelector
cdi:TextPositionSelector
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "TextPositionSelector";
  rdfs:comment "Definition\n==========\nDescribes a range of text by recording the start and end positions of the selection in the object. Position 0 would be immediately before the first character, position 1 would be immediately before the second character, and so on. It is defined accordingly the related selector of the Web Annotation Vocabulary, see https://www.w3.org/TR/annotation-vocab/#textpositionselector."@en;
  rdfs:subClassOf cdi:Selector;
  owl:hasKey (cdi:TextPositionSelector-end cdi:TextPositionSelector-start);
.

cdi:TextPositionSelector-end
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "end";
  rdfs:comment "Position of the last character of the selection. Position 8 would be the end of the word \"Position\" of the previous sentence."@en;
  rdfs:domain cdi:TextPositionSelector;
  rdfs:range xsd:integer;
.

cdi:TextPositionSelector-start
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "start";
  rdfs:comment "Position of the first character of the selection. Position 0 would be the start of the word \"Position\" of the previous sentence."@en;
  rdfs:domain cdi:TextPositionSelector;
  rdfs:range xsd:integer;
.


# datatype TimeRole
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::TimeRole
cdi:TimeRole
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "TimeRole";
  rdfs:comment "Definition\n============\nTime-specific role given to a represented variable in the context of a data structure. The specific characterization of the role (e.g. event, valid, transaction, reference, etc.) may be given by a controlled vocabulary."@en;
  rdfs:subClassOf cdi:SpecializationRole;
  owl:hasKey (cdi:TimeRole-time);
.

cdi:TimeRole-time
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "time";
  rdfs:comment "Holds a value from an external controlled vocabulary defining the time role."@en;
  rdfs:domain cdi:TimeRole;
  rdfs:range cdi:ControlledVocabularyEntry;
.


# datatype TypedString
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::TypedString
cdi:TypedString
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "TypedString";
  rdfs:comment "Definition\n============\nTypedString combines a type with content defined as a simple string. May be used wherever a simple string needs to support a type definition to clarify its content.\n\nExamples\n==========\nContent is a regular expression and the typeOfContent attribute is used to define the syntax of the regular expression content.\n\nExplanatory notes\n===================\nThis is a generic type + string where property name and documentation should be used to define any specification for the content. If international structured string content is required use TypedStructuredString."@en;
  
  owl:hasKey (cdi:TypedString-content cdi:TypedString-typeOfContent);
.

cdi:TypedString-content
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "content";
  rdfs:comment "Content of the property expressed as a simple string."@en;
  rdfs:domain cdi:TypedString;
  rdfs:range xsd:string;
.

cdi:TypedString-typeOfContent
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "typeOfContent";
  rdfs:comment "Optional use of a controlled vocabulary to specifically type the associated content."@en;
  rdfs:domain cdi:TypedString;
  rdfs:range cdi:ControlledVocabularyEntry;
.


# datatype WebLink
# based on the UML data type DDICDIModels::DDICDILibrary::DataTypes::StructuredDataTypes::WebLink
cdi:WebLink
  a rdfs:Class, owl:Class, ucmis:StructuredDataType;
  rdfs:label "WebLink";
  rdfs:comment "Definition\n============\nA web site (normally a URL) with information on type of site, privacy flag, and effective dates."@en;
  
  owl:hasKey (cdi:WebLink-effectiveDates cdi:WebLink-isPreferred cdi:WebLink-privacy cdi:WebLink-typeOfWebsite cdi:WebLink-uri);
.

cdi:WebLink-effectiveDates
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "effectiveDates";
  rdfs:comment "The period for which this URL is valid."@en;
  rdfs:domain cdi:WebLink;
  rdfs:range cdi:DateRange;
.

cdi:WebLink-isPreferred
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "isPreferred";
  rdfs:comment "Set to True if this is the preferred URL."@en;
  rdfs:domain cdi:WebLink;
  rdfs:range xsd:boolean;
.

cdi:WebLink-privacy
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "privacy";
  rdfs:comment "Indicates the privacy level of this URL."@en;
  rdfs:domain cdi:WebLink;
  rdfs:range cdi:ControlledVocabularyEntry;
.

cdi:WebLink-typeOfWebsite
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "typeOfWebsite";
  rdfs:comment "The type of Website URL, for example personal, project, organization, division, etc."@en;
  rdfs:domain cdi:WebLink;
  rdfs:range cdi:ControlledVocabularyEntry;
.

cdi:WebLink-uri
  a rdf:Property, owl:DatatypeProperty, ucmis:Attribute;
  rdfs:label "uri";
  rdfs:comment "A Uniform Resource Identifier (URI) is a compact sequence of characters that identifies an abstract or physical resource. Normally a URL."@en;
  rdfs:domain cdi:WebLink;
  rdfs:range xsd:anyURI;
.




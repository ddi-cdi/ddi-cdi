  rdfs:label "effectiveDates";
  rdfs:comment "The period for which this image is effective/valid."@en;
  rdfs:domain cdi:PrivateImage;
  rdfs:range cdi:DateRange;
.

cdi:PrivateImage-privacy
  a rdf:Property, owl:ObjectProperty, ucmis:Attribute;
  rdfs:label "privacy";
  rdfs:comment "Specify the level privacy for the image as public, restricted, or private. Supports the use of an external controlled vocabulary."@en;
  rdfs:domain cdi:PrivateImage;
  rdfs:range cdi:ControlledVocabularyEntry;
.


# datatype ProvenanceInformation
